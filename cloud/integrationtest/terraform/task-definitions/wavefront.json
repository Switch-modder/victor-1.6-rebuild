[
    {
        "name": "wavefront",
        "image": "wavefronthq/proxy",
        "environment": [
            {
                "name": "WAVEFRONT_URL",
                "value": "${wavefront_url}"
            },
            {
                "name": "WAVEFRONT_TOKEN",
                "value": "${wavefront_token}"
            },
            {
                "name": "JAVA_HEAP_USAGE",
                "value": "1g"
            }
        ],
        "portMappings": [
            {
              "containerPort": 2878,
              "hostPort": 2878
            }
        ]
    }
  ]