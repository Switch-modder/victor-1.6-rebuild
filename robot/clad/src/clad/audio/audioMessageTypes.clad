
namespace Anki {
namespace AudioEngine {
namespace Multiplexer {


enum uint_8 AudioCallbackFlag
{
  EventNone     = 0x00,
  EventDuration = 0x01,
  EventMarker   = 0x02,
  EventComplete = 0x04,
  EventAll      = 0x07,
  EventError    = 0xff,
}


enum uint_8 CurveType
{
  Linear = 0,       // Linear (Default)
  SCurve,           // S Curve
  InversedSCurve,   // Inversed S Curve
  Sine,             // Sine Curve
  SineReciprocal,   // Reciprocal of Sine Curve
  Exp1,             // Exponential 1 Curve
  Exp3,             // Exponential 3 Curve
  Log1,             // Logarithm 1 Curve
  Log3              // Logarithm 3 Curve
}

} // Multiplexer
} // AudioEngine
} // Anki
